!!!
%html
  %head
    %meta{ charset: "utf-8" }
    %meta{ content: "IE=edge,chrome=1", http: { equiv: "X-UA-Compatible" } }
    %meta{ name: "viewport", content: "width=device-width, initial-scale=1" }
    %title Unique Characters
    = favicon_tag "favicon.png"
    = stylesheet_link_tag "application"
    = partial "partials/theme_styles"

  %body
    .theme-gold{ data: { theme_class: true } }
      = partial "partials/info"
      .main-wrapper
        .main-overlay{ data: { main_overlay: true, info_hide: true } }
        = partial "partials/filter_nav"
        %main
          = partial "partials/header"
          = yield

    = partial "partials/theme_dots"

    = javascript_include_tag "https://code.jquery.com/jquery-2.1.1.min.js"
    = javascript_include_tag "https://cdn.jsdelivr.net/npm/js-cookie@2/src/js.cookie.min.js"
    = javascript_include_tag "application"
    = partial "partials/google_analytics"

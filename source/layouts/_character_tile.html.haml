.character-tile-wrapper
  = link_to article.data.website_url, class: "character-tile", target: "_blank" do
    .character-tile__character
      = svg "#{article.date.strftime('%Y-%m-%d')}"
    .character-tile__meta
      .character-tile__time= article.date.strftime('%m.%d.%Y')
      .character-tile__typeface= article.title
    .character-tile__arrow
      = material_icon("call_made")
